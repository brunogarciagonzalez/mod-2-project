<!DOCTYPE html>
<html>
  <head>
    <title>Mod2Project</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<%= link_to "Listings Index", listings_path %>
  	<%= link_to "Users Index", users_path %>
    <%= link_to "Orders Index", orders_path %>

    <div style="display: inline-block; float:right">
  	<%= logged_in? ? link_to("Sign Out", '/signout') : link_to("Sign In", '/signin') %>
    <%= if !logged_in? then link_to("Create New Account", '/register') end%>
    </div>
    <br />
    
    <%= yield %>
  </body>
</html>
